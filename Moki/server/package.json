{
  "name": "moki-express",
  "version": "1.1.0",
  "description": "Moki express server",
  "author": "Frafos, Intuitive Labs",
  "type": "module",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "homepage": "https://www.frafos.com/  https://www.intuitivelabs.com/",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "dev": "tsx watch src/app.ts",
    "test": "vitest",
    "testlab": "NODE_ENV=CI vitest run --coverage --reporter=verbose --reporter=junit --outputFile=report.xml",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ignore-path .gitignore ./src/",
    "lintlab": "eslint --fix --ignore-path .gitignore ./src/ --format gitlab",
    "swagger": "swagger-jsdoc -d ./src/modules/swagger.js ./src/**/*.js",
    "license-checker": "license-checker",
    "ncu": "ncu"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.7.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "eslint-formatter-gitlab": "^4.0.0",
    "express": "^4.18.2",
    "express-prettify": "^0.1.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "process": "^0.11.10",
    "sockjs-client": "^1.6.1",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitest/coverage-istanbul": "^0.32.0",
    "eslint": "^8.39.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "license-checker": "^25.0.1",
    "nyc": "^15.1.0",
    "tsc-alias": "^1.8.6",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  }
}
